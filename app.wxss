.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.left-column{
  width:25%;
  text-align: right;
  font-size: 10pt;
  margin-top:20rpx;
}
.right-column{
  width:75%;
  text-align: left;
  font-size: 10pt;
  margin-top:20rpx;
}
